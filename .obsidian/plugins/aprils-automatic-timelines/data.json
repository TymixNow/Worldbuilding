{
  "dateDisplayFormat": "{day}.{month}.{year} {age}",
  "dateParserGroupPriority": "age,year,month,day",
  "dateParserRegex": "(?<age>-?[0-9]*)-(?<year>-?[0-9]*)-(?<month>-?[0-9]*)-(?<day>-?[0-9]*)",
  "eventRenderToggleKey": "render?",
  "inlineEventEndOfBodyMarker": "%%end-of-body%%",
  "markdownBlockTagsToFindSeparator": ",",
  "metadataKeyEventBodyOverride": "body",
  "metadataKeyEventEndDate": "end-date",
  "metadataKeyEventPictureOverride": "picture",
  "metadataKeyEventStartDate": "start-date",
  "metadataKeyEventTimelineTag": "timelines",
  "metadataKeyEventTitleOverride": "event-title",
  "noteInlineEventKey": "inline-event",
  "ignoreLineCommentToken": "aat-ignore-line",
  "applyAdditonalConditionFormatting": true,
  "lookForCalendariumSpanEvents": true,
  "lookForInlineEventsInNotes": true,
  "lookForTagsForTimeline": false,
  "stylizeDateInline": false,
  "bodyFontSize": -1,
  "dateFontSize": 10,
  "titleFontSize": 20,
  "dateTokenConfiguration": [
    {
      "minLeght": 4,
      "name": "year",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    },
    {
      "minLeght": 2,
      "name": "month",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    },
    {
      "minLeght": 2,
      "name": "day",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    },
    {
      "minLeght": 2,
      "name": "age",
      "type": "STRING",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false,
      "dictionary": [
        "α",
        "μ",
        "Ω"
      ]
    }
  ]
}